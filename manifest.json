{
  "manifest_version": 2,

  "name": "calC",
  "description": "A beautiful calculator and converter.",
  "version": "1.1",
  "icons": { 
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png" 
  },
  "content_scripts": [{
    "matches": ["http://oruapp.co/*"],
     "js": ["math.min.js", "script.min.js"]
   }],
  "browser_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_popup": "calc.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage"
  ]
}